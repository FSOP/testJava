Class DbReportCat extends XmlBottom {
String nations[] = ['US', 'SKOR'];
 public void reportCat(){
  JSONObject current-cat = new JSONObject();
  JSONObject diff-cat = new JSONObject;
  JSONObjectyesterday-cat = get-yesterday-cat();


  String query = "select nation, count(*) from satcat where group by nations";
  
  Resultset res = state.executeQuery(query);

   While(res.next()){
      String nation = res.getString(1);
      int val = res.getInt(2);
      
      current-cat.put(nation, val);
    }

   for (String nat: nations){
     diff-cat.put();
   }
  }


  public HashMap get-yesterday-cat(){
    initDB();
    String query-yesterday = "select insertdate, summary-data from cat-summary order by insertdate desc limit 1";
    
    ResultSet res = state.executeQuery(query);
    String val = res.getString(2);

    JSONObject jobj = parser.parse(val);

    closeConn();
    return jobj
  }

}
